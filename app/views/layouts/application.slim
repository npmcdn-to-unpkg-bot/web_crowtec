doctype html
html
  head
    title Crowtec

    meta name="viewport" content="width=device-width, initial-scale=1"
    = stylesheet_link_tag "https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.6.0/slick.min.css", media: 'all', 'data-turbolinks-track' => true
    = stylesheet_link_tag "https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.6.0/slick-theme.min.css", media: 'all', 'data-turbolinks-track' => true
    = stylesheet_link_tag "https://cdnjs.cloudflare.com/ajax/libs/normalize/4.2.0/normalize.min.css", media: 'all', 'data-turbolinks-track' => true
    = stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true

    = javascript_include_tag 'application', 'data-turbolinks-track' => true
    = javascript_include_tag 'https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.6.0/slick.min.js', 'data-turbolinks-track' => true
    = javascript_include_tag 'https://unpkg.com/masonry-layout@4.1/dist/masonry.pkgd.min.js', 'data-turbolinks-track' => true
    = javascript_include_tag 'https://npmcdn.com/imagesloaded@4.1/imagesloaded.pkgd.min.js', 'data-turbolinks-track' => true
    = csrf_meta_tags
  body
    = render "partials/application/navigation"
    = yield
    = render "partials/application/footer"


